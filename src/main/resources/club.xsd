    <xs:element name="getClubRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name ="id" type="xs:long"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="getClubResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name ="clubDto" type="tns:clubDto"/>
                <xs:element name="statusMessage" type="tns:statusMessage"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>


    <xs:element name="getAllClubsRequest">
        <xs:complexType>
            <xs:sequence>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="getAllClubsResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="clubDtos"
                            type="tns:clubDto"
                            minOccurs="0"
                            maxOccurs="unbounded"/>
                <xs:element name="statusMessage" type="tns:statusMessage"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>


    <xs:element name="createClubRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="createClubDto" type="tns:createClubDto"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="createClubResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="clubDto" type="tns:clubDto"/>
                <xs:element name="statusMessage" type="tns:statusMessage"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>


    <xs:element name="updateClubRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:long"/>
                <xs:element name="createClubDto" type="tns:createClubDto"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="updateClubResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="clubDto" type="tns:clubDto"/>
                <xs:element name="statusMessage" type="tns:statusMessage"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="deleteClubRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:long"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="deleteClubResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="statusMessage" type="tns:statusMessage"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

</xs:schema>